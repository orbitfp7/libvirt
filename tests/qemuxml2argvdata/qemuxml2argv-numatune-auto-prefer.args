LC_ALL=C \
PATH=/bin \
HOME=/home/test \
USER=test \
LOGNAME=test \
QEMU_AUDIO_DRV=none \
/usr/bin/kvm \
-name QEMUGuest \
-S \
-M pc \
-m 64 \
-smp 1 \
-numa node,nodeid=0,cpus=0,mem=64 \
-uuid 9f4b6512-e73a-4a25-93e8-5307802821ce \
-nographic \
-nodefaults \
-monitor unix:/tmp/test-monitor,server,nowait \
-no-acpi \
-boot c \
-usb \
-device virtio-balloon-pci,id=balloon0,bus=pci.0,addr=0x3
