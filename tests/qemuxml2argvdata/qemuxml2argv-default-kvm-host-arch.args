LC_ALL=C \
PATH=/bin \
HOME=/home/test \
USER=test \
LOGNAME=test \
QEMU_AUDIO_DRV=none \
/usr/bin/kvm \
-name kvm \
-S \
-machine pc,accel=kvm \
-m 4096 \
-smp 4 \
-uuid d091ea82-29e6-2e34-3005-f02617b36e87 \
-nographic \
-nodefaults \
-monitor unix:/tmp/test-monitor,server,nowait \
-no-acpi \
-boot c \
-usb \
-device virtio-balloon-pci,id=balloon0,bus=pci.0,addr=0x3
